{
    // "python.linting.pylintEnabled": true,
    // "python.linting.enabled": true
    "editor.suggest.insertMode": "replace",
    "git.ignoreLimitWarning": true,
    "python.linting.pylintEnabled": true,
    // "python.pythonPath": "../../Scripts/python.exe",
    // "python.pythonPath": "C:\\ProgramData\\Anaconda3\\envs\\djangoSite2\\python.exe",
    // "python.linting.pylintEnabled": true,
    "python.linting.pylintArgs": [
        "--enable=F,E,W",
        "--load-plugins=pylint_django",
        "pylint_django",
        // "--disable=C0111",
        "--load-plugins",
    ],
    "python.formatting.blackArgs": [
        "--line-length=140",
    ],
    "python.formatting.provider": "black",
    "python.linting.enabled": true,
    // "cornflakes.linter.executablePath": "C:/Users/VISTA FOUNDATION/.virtualenvs/testing_course-6y64xQe1/Scripts/Scripts/python.exe",
    "cornflakes.linter.run": "onType",
    "python.linter": "flake8",
    "python.linting.banditEnabled": false,
    "python.linting.flake8Enabled": true,
    "python.linting.pycodestyleEnabled": true,
    "python.linting.pycodestyleArgs": [
        "--ignore=E501,W504"
    ],
    "python.linting.flake8Args": [
        "--max-line-length=120",
        "--ignore=E402,F841,F401,E302,E305,E501,E241,E303,W504",
    ],
    "editor.formatOnSave": true,
    "editor.formatOnSaveMode": "file",
    "python.linting.lintOnSave": false,
    "[python]": {
        "editor.tabSize": 4,
        "editor.insertSpaces": true,
        "editor.formatOnSave": true
    },
    "editor.quickSuggestions": {
        "other": true,
        "comments": false,
        "strings": false,
    },
    "editor.acceptSuggestionOnEnter": "smart",
    "editor.suggest.showIcons": true,
    "editor.suggest.snippetsPreventQuickSuggestions": true,
    // "editor.suggest.insertMode": "insert",
    "editor.suggest.showClasses": true,
    "editor.suggest.showSnippets": true,
    "editor.snippetSuggestions": "bottom",
    "cSpell.words": [
        "Amazonn",
        "cond",
        "febo",
        "jqoezr",
        "nada"
    ],
    "jupyter.debugJustMyCode": true,
    "debug.allowBreakpointsEverywhere": true,
}
